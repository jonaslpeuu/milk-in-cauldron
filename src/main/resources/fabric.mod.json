  "authors": [
    "whitoutcookies"
  ],
  "contact": {
    "homepage": "https://modrinth.com/mod/milk-in-cauldron",
    "sources": "https://github.com/whitoutcookies/milk-in-cauldron",
    "issues": "https://github.com/whitoutcookies/milk-in-cauldron/issues"
  },
  "license": "All-Rights-Reserved",
  "icon": "assets/milk_in_cauldron/icon.png",
  "environment": "*",
  "entrypoints": {
    "fabric-datagen": [
      "de.whitoutcookies.milk_in_cauldron.client.Milk_in_cauldronDataGenerator"
    ],
    "client": [
      "de.whitoutcookies.milk_in_cauldron.client.Milk_in_cauldronClient"
    ],
    "main": [
      "de.whitoutcookies.milk_in_cauldron.Milk_in_cauldron"
    ]
  },
  "mixins": [
    "milk_in_cauldron.mixins.json",
